
@using BackOffice.Translates
@model BackOffice.Areas.LykkePay.Models.StaffsIndexViewModel
<ul id="tabs" class="nav nav-tabs">
    <li id="staffs"><a data-toggle="tab" onclick="selectTab(this.parentNode.id, '@Url.Action("StaffsPage")')">Portal Access</a></li>
</ul>

<div id="StaffsPage" style="margin: 10px"></div>

<script>
    function selectTab(tabId, url) {
        if ($('#' + tabId).hasClass('active'))
            return;
        $('#tabs').children('li').each(function() {
            (this.id === tabId) && $('#' + this.id).addClass('active') || $('#' + this.id).removeClass('active');
        });
        requests.doRequest({ url: url, divResult: '#StaffsPage', params: { merchant: '@Model.MerchantId'} });
    }
    selectTab('staffs', '@Url.Action("StaffsPage")');
</script>